<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".LocationDetailActivity">

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="105dp"
        android:layout_marginBottom="8dp"
        android:text="Name:"
        app:layout_constraintBottom_toTopOf="@+id/textView_characterDetail_name"
        app:layout_constraintStart_toStartOf="@+id/textView_characterDetail_name"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="7dp"
        android:text="Type:"
        app:layout_constraintBottom_toTopOf="@+id/textView_locationDetail_type"
        app:layout_constraintEnd_toEndOf="@+id/textView_locationDetail_type"
        app:layout_constraintTop_toBottomOf="@+id/textView_characterDetail_name" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="10dp"
        android:text="Dimension:"
        app:layout_constraintBottom_toTopOf="@+id/textView_characterDetail_dimension"
        app:layout_constraintStart_toStartOf="@+id/textView_locationDetail_type"
        app:layout_constraintTop_toBottomOf="@+id/textView_locationDetail_type" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginBottom="9dp"
        android:text="Created:"
        app:layout_constraintBottom_toTopOf="@+id/textView_characterDetail_created"
        app:layout_constraintStart_toStartOf="@+id/textView_characterDetail_dimension"
        app:layout_constraintTop_toBottomOf="@+id/textView_characterDetail_dimension" />

    <TextView
        android:id="@+id/textView_characterDetail_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="105dp"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/textView2"
        app:layout_constraintEnd_toEndOf="@+id/textView_locationDetail_type"
        app:layout_constraintStart_toStartOf="@+id/textView_locationDetail_type"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <TextView
        android:id="@+id/textView_characterDetail_dimension"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="83dp"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/textView4"
        app:layout_constraintStart_toStartOf="@+id/textView3"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <TextView
        android:id="@+id/textView_characterDetail_created"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginBottom="188dp"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/textView4"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <TextView
        android:id="@+id/textView_locationDetail_type"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="165dp"
        android:layout_marginBottom="65dp"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/textView3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

</androidx.constraintlayout.widget.ConstraintLayout>